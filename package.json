{
  "author": "Jan Hesters",
  "dependencies": {
    "ramda": "0.27.1"
  },
  "description": "A simple mock function to mock functions.",
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/ramda": "0.27.33",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "23.0.0",
    "husky": "4.3.5",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "riteway": "6.2.1",
    "tap-nirvana": "1.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "license": "MIT",
  "main": "dist/index.js",
  "name": "fn",
  "private": false,
  "repository": "https://github.com/janhesters/fn",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache . --ext ts --ext tsx --ext js",
    "test": "NODE_ENV=test riteway -r ts-node/register/transpile-only 'src/**/*.test.ts' | tap-nirvana",
    "type-check": "tsc --pretty --noEmit"
  },
  "version": "1.0.0"
}
